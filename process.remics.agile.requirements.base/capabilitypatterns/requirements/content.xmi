  <org.eclipse.epf.uma:DescriptorDescription xmi:id="-vabmS_tjZE_PqCGbgg-AxQ" name="sprint_goal,_QE4Xcf_7EeKZBbwDq57ZRA" guid="-vabmS_tjZE_PqCGbgg-AxQ">
    <refinedDescription>&lt;h4>&#xD;
