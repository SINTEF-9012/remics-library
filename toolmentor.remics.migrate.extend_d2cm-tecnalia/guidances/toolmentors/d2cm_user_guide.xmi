        &lt;span lang=&quot;EN-US&quot;>e.&lt;span style=&quot;FONT: 7pt 'Times New Roman'&quot;>&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;/span>&lt;/span> &lt;span&#xD;
        lang=&quot;EN-US&quot;>Start-up command(s) executed then the virtual machine boots,&lt;/span>&#xD;
