            &lt;tr>&#xD;
                &lt;td height=&quot;14&quot; width=&quot;2&quot;>&#xD;
                &lt;/td>&#xD;
            &lt;/tr>&#xD;
            &lt;tr>&#xD;
                &lt;td>&#xD;
                &lt;/td>&#xD;
                &lt;td>&#xD;
                    &lt;img&#xD;
                    alt=&quot;Cuadro de texto: self.eAttributes-&amp;gt;select(a | a.eType.name = 'Boolean')-&amp;gt;collect(a | Tuple {&#xD;&#xD;&#xD;
                    	scope = self,&#xD;&#xD;&#xD;
                    	nameFragments = Sequence{'Is', a.name,self.name},&#xD;&#xD;&#xD;
                    	operationFragments = Sequence{'self.', a.name , ' = true'}&#xD;&#xD;&#xD;
                    }&#xD;&#xD;&#xD;
                    )&#xD;&#xD;&#xD;
                    &quot; src=&quot;./resources/metrinouserguide032.gif&quot; width=&quot;604&quot; height=&quot;174&quot; />&#xD;
                &lt;/td>&#xD;
            &lt;/tr>&#xD;
